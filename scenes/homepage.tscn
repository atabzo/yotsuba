script/source = "extends Button

# Path to the scene you want to load
@export var scene_path: String = \"res://scenes/opening.tscn\"

func _ready():
	# Connect the button's pressed signal to our function
	pressed.connect(_on_button_pressed)

func _on_button_pressed():
	# Change to the specified scene
	get_tree().change_scene_to_file(scene_path)
"

[sub_resource type="GDScript" id="GDScript_m75kp"]
script/source = "extends Sprite2D

# Animation settings
@export var rotation_angle: float = 15.0  # Degrees to rotate (each direction)
@export var animation_speed: float = 2.0  # Speed of the animation
@export var random_offset: bool = true    # Start at random point in animation

var time_passed: float = 0.0

func _ready():
	# Start at a random point in the animation cycle
	if random_offset:
		time_passed = randf() * TAU  # TAU = 2 * PI

func _process(delta):
	time_passed += delta * animation_speed
	
	# Calculate rotation using sine wave for smooth back-and-forth motion
	var angle = sin(time_passed) * deg_to_rad(rotation_angle)
	rotation = angle
"

